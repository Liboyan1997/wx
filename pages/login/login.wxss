/* pages/login.wxss */
.container{
  overflow: hidden;
}
.logo{
  height: 250rpx;
  width: 250rpx;
  margin: 50rpx auto;
  display: flex;
  flex-direction:column;
  align-items: center;
}
.logo>image{
  height: 150rpx;
  width: 150rpx;
  display: inline-block;
  margin: 10rpx 0;
}
.footer{
  display: flex;
  flex-direction: column;
  align-items: center;
  font-size: 28rpx;
  color: darkgray;

}
.footer>button::after{
  border: none;
}
.footer>button{
  margin: 200rpx 0 30rpx 0;
  border: none;
  outline: none;
  display: block;
}
.footer image{
  display: block;
  border: none ;
  width: 120rpx;
  height: 120rpx;
}